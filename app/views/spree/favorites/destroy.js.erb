<% if @success %>
  if($("#favorable_<%= @favorable_type.demodulize.underscore %>_<%= @favorable_id %>").length) {
    $("#favorable_<%= @favorable_type.demodulize.underscore %>_<%= @favorable_id %>").show();
		$("favorite-icon").show();
  } else {
    $("#favorite_<%= @favorite.id %>").hide();
		$("favorite-icon").hide();
  }
<% end %>
